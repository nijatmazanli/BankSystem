<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Prosperity Bank</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <style>
        h1 {
            text-align: center;
        }

        .card {
            min-height: 300px;
        }

        .footer {
            position: fixed;
            left: 0;
            bottom: 0;
            width: 100%;
            text-align: center;
        }

        img:hover {
            rotate: 1800deg;
            transition: all 10s ease-in-out;
        }

        img {
            rotate: 0deg;
            transition: all 10s ease-in-out;
        }
    </style>
</head>
<body>
<!-- Navigation Bar -->
<nav class="navbar navbar-expand-lg navbar-light bg-light">
    <a class="navbar-brand" href="#">
        <img src="../assets/img/logoStarryNight2.png" width="100" height="100" class="d-inline-block align-top" alt="">
    </a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
            aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
            <li class="nav-item ">
                <a class="nav-link" href="/">Home </a>
            </li>
            <li class="nav-item active">
                <a class="nav-link" href="/features">Features <span class="sr-only">(current)</span></a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/pricing">Pricing</a>
            </li>

        </ul>
    </div>
</nav>

<!-- Main Content Section with Image Carousel -->
<div class="container mt-4">
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">

            <div class="carousel-item active">
                <h1>Online Banking Features</h1>
                <div class="card-columns">

                    <% let i = -1; names.forEach(function(name) { i++; %>
                    <div class="card">
                        <div class="card-body border-dark ">
                            <div class="card-header"><%= name %></div>
                            <div class="card-body text-dark">
                                <h5 class="card-title">Dark card title</h5>
                                <p class="card-text"><%= desc[i] %></p>
                            </div>
                        </div>
                    </div>
                    <% }); %>
                </div>
            </div>
            <div class="carousel-item ">
                <h1>CashBack Card Features</h1>
                <div class="card-columns">

                    <% let f = -1; cashbackname.forEach(function(name) { f++; %>
                    <div class="card">
                        <div class="card-body border-dark ">
                            <div class="card-header"><%= name %></div>
                            <div class="card-body text-dark">
                                <h5 class="card-title">Dark card title</h5>
                                <p class="card-text"><%= cashbackdescription[f] %></p>
                            </div>
                        </div>
                    </div>
                    <% }); %>
                </div>
            </div>
            <div class="carousel-item ">
                <h1>Credit Card Features</h1>
                <div class="card-columns">

                    <% let g = -1; creditcardname.forEach(function(name) { g++; %>
                    <div class="card">
                        <div class="card-body border-dark ">
                            <div class="card-header"><%= name %></div>
                            <div class="card-body text-dark">
                                <h5 class="card-title">Dark card title</h5>
                                <p class="card-text"><%= creditcarddesc[g] %></p>
                            </div>
                        </div>
                    </div>
                    <% }); %>
                </div>
            </div>
            <a class="carousel-control-prev fg-light " href="#carouselExampleIndicators" role="button"
               data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
</div>
<!-- Footer -->
<footer class="footer bg-light text-center text-lg-start">
    <div class="container p-4">
        <div class="text-center p-3">
            Â© 2025 Simple Bootstrap Page:
            <a class="text-dark" href="#">Your Website</a>
        </div>
    </div>
</footer>

<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.3/dist/umd/popper.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>

</body>
</html>
